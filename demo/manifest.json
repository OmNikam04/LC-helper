{
    "manifest_version": 3,
    "name": "Code Generator Extension",
    "version": "1.0",
    "description": "Generate code from HTML data.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }
  